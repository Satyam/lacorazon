    "description": "Administración ventas libro La Corazón",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.12",
        "@fortawesome/free-solid-svg-icons": "^5.6.3",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "bootstrap": "^4.1.3",
        "classnames": "^2.2.6",
        "firebase": "^5.7.1",
        "formik": "^1.4.1",
        "immer": "^3.1.1",
        "react": "^16.8.6",
        "react-datepicker": "^2.0.0",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0-alpha.1",
        "react-router-dom": "^5.0.0",
        "reactstrap": "^8.0.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "use-react-router": "^1.0.2",
        "yup": "^0.27.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "upload": "node ./migrate/upload.js",
        "analyze": "source-map-explorer build/static/js/main.* & source-map-explorer build/static/js/runtime*.* & source-map-explorer build/static/js/1.*",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "test:debug": "react-scripts --inspect-brk test --runInBand",
        "coverage": "react-scripts test --coverage",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build",
        "docs": "documentation serve src/store/**  --watch --document-exported --sort-order alpha",
        "docs:build": "documentation build src/store/** -f html -o docs --document-exported --sort-order alpha"
    },
    "author": "Daniel Barreiro (Satyam)",
    "license": "MIT",
    "repository": "https://github.com/Satyam/lacorazon",
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "documentation": "^10.0.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.12.1",
        "enzyme-to-json": "^3.3.5",
        "firebase-admin": "^7.3.0",
        "prop-types": "^15.6.2",
        "rc-util": "^4.6.0",
        "react-scripts": "^3.0.0",
